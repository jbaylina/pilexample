constant %N = 1024;

namespace Fibonacci(%N);
    pol constant ISLAST;             // 0,0,0,0,.....,1
    pol commit aBeforeLast, aLast;

    (1-ISLAST) * (aBeforeLast' - aLast) = 0;
    (1-ISLAST) * (aLast' - (aBeforeLast + aLast)) = 0;

    public out = aLast(%N-1);
    ISLAST * (aLast - :out) = 0;
